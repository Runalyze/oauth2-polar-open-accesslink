{
    "name": "runalyze/oauth2-polar-open-accesslink",
    "description": "Polar Open Accesslink OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Pohl",
            "email": "michael@mipapo.de"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "polarOpenAccessLink",
        "polarFlow"
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
